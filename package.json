{
  "name": "cloud-score",
  "description": "Cloud score of the biggest cloud providers",
  "version": "1.0.0",
  "author": "Gabriel Nepomuceno <gabriel.nepomuceno@microsoft.com>",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run sass && npm run build:parts && npm run clean:css && npm run process:html",
    "build:parts": "npm run sass && npm run compile",
    "clean": "rimraf dist && rimraf coverage",
    "clean:css": "cleancss -o dist/css/main.min.css src/css/bootstrap.css src/css/main.css",
    "compile": "webpack --config config/webpack.config.prod.js",
    "copy:bs": "ncp node_modules/bootstrap/dist/css/bootstrap.css src/css/bootstrap.css",
    "dev": "cross-env NODE_ENV=development npm run sass && concurrently --kill-others \"webpack-dev-server --hot --inline\" \"node-sass --watch --output ./src/css ./src/sass\"",
    "lint": "tslint src/**/*.ts",
    "process:html": "htmlprocessor src/index.html -o dist/index.html",
    "sass": "node-sass --output ./src/css ./src/sass",
    "serve": "http-server dist/ -o",
    "test": ""
  },
  "dependencies": {
    "@types/jquery": "^3.2.8",
    "@types/plotly.js": "^1.28.4",
    "axios": "~0.16.2",
    "documentdb": "^1.12.0",
    "google-trends-api": "^4.1.0",
    "plotly.js": "git+https://github.com/arlowhite/plotly.js#no-polyfill",
    "vue": "~2.4.1",
    "vue-class-component": "~5.0.2",
    "vue-materials": "^1.0.7-rc.7",
    "vue-property-decorator": "~5.1.1",
    "vue-router": "~2.7.0"
  },
  "devDependencies": {
    "@types/mocha": "~2.2.41",
    "@types/node": "~8.0.13",
    "@types/sinon": "~2.3.2",
    "awesome-typescript-loader": "~3.2.1",
    "clean-css-cli": "~4.1.6",
    "compression-webpack-plugin": "~1.0.0",
    "concurrently": "~3.5.0",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~5.0.1",
    "es6-promise": "~4.1.1",
    "file-loader": "~0.11.2",
    "globby": "~6.1.0",
    "htmlprocessor": "~0.2.4",
    "http-server": "~0.10.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "json-loader": "~0.5.4",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~3.4.2",
    "ncp": "~2.0.0",
    "node-sass": "^4.5.3",
    "opn-cli": "~3.1.0",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.5",
    "rimraf": "~2.6.1",
    "sinon": "~2.3.8",
    "tslint": "~5.5.0",
    "tslint-loader": "~3.5.3",
    "typescript": "~2.4.1",
    "webpack": "~3.3.0",
    "webpack-dev-server": "~2.5.1"
  }
}
